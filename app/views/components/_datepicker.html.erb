<div class="RRow">
    <label tabindex="0" class="RDatePicker__input">
        <label class="RInput default eluno readonly">
            <div class="RInput__body is-label">
                <label class="RInput__input">
                    <input type="text" disabled="disabled" autocomplete="new-password">
                    <label class="RInput__label">Label text</label>
                </label>
            </div>
            <a class="RInput__after events">
                <i class="RIcon rir-calendar_16"></i>
            </a>
        </label>
    </label>

    <div class="RDatePicker">
        <div class="RDatePicker__navigation">
            <button tabindex="0" class="RButtonAction rocky sulguni">
                <i class="RIcon RButtonAction__icon rir-rewind_16"></i>
            </button>
            <button tabindex="0" class="RButtonAction rocky sulguni">
                <i class="RIcon RButtonAction__icon rir-arrow-left_16"></i>
            </button>
            <button tabindex="0" class="RButtonAction rocky sulguni">
                <i class="RIcon RButtonAction__icon rir-arrow-right_16"></i>
            </button>
            <button tabindex="0" class="RButtonAction rocky sulguni">
                <i class="RIcon RButtonAction__icon rir-forward_16"></i>
            </button>
            <span class="RDatePicker__title month"></span>
            <span class="RDatePicker__title year"></span>
        </div>
        <div class="RDatePicker__week">
        </div>
        <div class="RDatePicker__calendar">
        </div>
    </div>


    <div id="date_picker">
        <div id="date_picker_input">
            <label>Datepicker</label>
            <input type="text" id="date" class="onFocus" readonly/>
        </div>
        <div id="date_picker_calendar" class="hidden">
            <div id="calendar_header">
                <button class="cal-btn back">
                    <img src="data:image/png;base64,"></button>
                <span></span>
                <button class="cal-btn front">
                    <img src="data:image/png;base64,"></button>
            </div>
            <div id="cal_wrapper">
                <div id="cal_days"></div>
                <div id="calendar_main"></div>
            </div>
        </div>
    </div>

</div>
